<link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" />


<script type="text/ng-template" id="/ui-tree-select.html">
    <ui-select ng-model="model.$selected"
               ui-select-focuser
               theme="selectize">
        <ui-select-match placeholder="Enter text">
            {{ $select.selected.title }}
        </ui-select-match>
        <ui-select-choices repeat="group in groups | filter: $select.search">
            <div>
                <span ng-bind-html="group.title | highlight: $select.search"></span>
                <!--<span ng-bind-html="'('+group.size+' users)'"></span>
                <a href
                  ng-show="group.parent"
                  class="goto-child-group"
                  ng-click="loadChildGroupsOf(group, $select)">
                  <i class="fa fa-angle-right"></i>
                </a>-->
            </div>
        </ui-select-choices>
    </ui-select>
</script>



<div ng-init="model = {}">



    <div class="content">
        <div class="panel lightblue bluebrdr marginbottom">
            <div class="panel-heading">
                <strong class="heading">Suppliers</strong>

                <div class="btn-group pull-right">
                    <button type="button" class="btn btn-primary btn-sm" @*ng-click="NewCustomerCreate();"*@ id="NewCustomerCreate"> New Supplier</button>
                    <button type="button" class="btn btn-primary btn-sm dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <span class="caret"></span>
                    </button>
                    <ul class="dropdown-menu">
                        <li><a href="#/Customer/Import">Import Suppliers</a></li>

                    </ul>
                </div>

            </div>

        

            <div class="panel-body statusBody">
                <div class="col-sm-2 cursor padding">

                    <div class="col-sm-12 estimate" ng-click="SuppliersTablebtn()" style="background:#e91e63">
                        <span class="rupee">&nbsp;</span>
                        <p class="text-uppercase marginbottom status" >{{suppliersCount.count}} suppliers</p>
                    </div>

                </div>


                <div class="col-sm-2 cursor padding">

                    <div class="col-sm-12 estimate" ng-click="EnquiryTablebtn()">
                        <span class="rupee">&nbsp;</span>
                        <p class="text-uppercase marginbottom status">{{enquiryCount.count}} Enquiry</p>
                    </div>

                </div>
                <div class="col-sm-2 cursor padding">

                    <div class="col-sm-12 estimate blue" ng-click="PurchaseOrderTable()">
                        <span class="rupee">Rs 50,000</span>
                        <p class="text-uppercase marginbottom status">3 Purchase Order</p>
                    </div>

                </div>
                <div class="col-sm-4 padding cursor">

                    <div class="col-sm-12 estimate bg-yellow padding" ng-click="OpenBillTable()">
                        <div class="col-sm-7" style="padding:10px">
                            <span class="rupee">Rs 10,000</span>
                            <p class="text-uppercase marginbottom status">5 OPEN BILL</p>
                        </div>
                        <div class="col-sm-5 estimate bg-red overduebox" ng-click="OverdueTable()">
                            <span class="rupee">Rs 50,000</span>
                            <p class="text-uppercase marginbottom status">3 overdue Bills</p>
                        </div>
                    </div>
                </div>
                <div class="col-sm-2 cursor padding">

                    <div class="col-sm-12 estimate bg-green" ng-click="PaidBillbtn()">
                        <span class="rupee">Rs 55,000</span>
                        <p class="text-uppercase marginbottom status">5 Bills Paid last 30 days</p>
                    </div>
                </div>
            </div>
        </div>
